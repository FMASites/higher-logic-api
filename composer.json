{
    "name": "fmasites/higher-logic-api",
    "version": "v1.0.7",
    "description": "Tools for interacting with Higher Logic",
    "type": "library",
    "authors": [
        {
            "name": "Kunzelman",
            "email": "mkunzelman@fmamfg.org"
        }
    ],
    "autoload": {
      "psr-4": {
        "FMASites\\HigherLogicApi\\": "src/"
      }
    },
    "require": {
      "php": ">=8.0 <8.5",
      "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
      "guzzlehttp/guzzle": "^7.0 || ^8.0"
    },
    "extra": {
      "laravel": {
        "providers": [
          "FMASites\\HigherLogicApi\\HigherLogicApiServiceProvider"
        ]
      }
    },
    "require-dev": {
      "phpunit/phpunit": "^11.0"
    },
    "autoload-dev": {
      "psr-4": {
        "FMASites\\HigherLogicApi\\Tests\\": "tests/"
      }
    }
}
