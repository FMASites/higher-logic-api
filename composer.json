{
    "name": "fmasites/higher-logic-api",
    "description": "Sends a request to be added to a mail list on HigherLogic",
    "type": "library",
    "authors": [
        {
            "name": "Kunzelman",
            "email": "mkunzelman@fmamfg.org"
        }
    ],
    "autoload": {
      "psr-4": {
        "HigherLogicApi\\": "src/"
      }
    },
    "require": {
      "php": "^8.0",
      "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
      "guzzlehttp/guzzle": "^7.0"
    },
    "extra": {
      "laravel": {
        "providers": [
          "HigherLogicApi\\HigherLogicApiServiceProvider"
        ]
      }
    },
    "require-dev": {
      "phpunit/phpunit": "^9.0"
    },
    "autoload-dev": {
      "psr-4": {
        "FMASites\\HigherLogicApi\\Tests\\": "tests/"
      }
    }
}
